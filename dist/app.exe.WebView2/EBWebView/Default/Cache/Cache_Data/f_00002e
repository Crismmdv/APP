    def count2(*args, **kwargs):
        check=list()
        sel=["Clase1","Clase2"]
        for i in sel:
            input_text = Element(i)    
            fin="a"+i
            op = Element(fin)
            if dtipos[input_text.value] == "object":
                key= "Válido"
                check.append(key)
            else: key = "Revisar valores"
            number = len(input_text.value)
