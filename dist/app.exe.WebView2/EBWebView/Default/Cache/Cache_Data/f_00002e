        elif len(check) != len((&#39;Cl&#39;, &#39;SO4&#39;, &#39;HCO3&#39;, &#39;CO3&#39;, &#39;Na&#39;, &#39;Ca&#39;, &#39;Mg&#39;, &#39;K&#39;, &#39;TDS&#39;)): document.getElementById("sgte").disabled = True
        
